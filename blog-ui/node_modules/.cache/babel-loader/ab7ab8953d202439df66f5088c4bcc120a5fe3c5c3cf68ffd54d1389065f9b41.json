\n        content: ''\n      }\n    };\n  },\n  methods: {\n    async fetchPosts() {\n      try {\n        const response = await this.$axios.get('http://localhost:3000/api/posts');\n        this.posts = response.data;\n      } catch (error) {\n        console.error('Failed to fetch posts:', error);\n      }\n    },\n    async createPost() {\n      try {\n        const response = await this.$axios.post('http://localhost:3000/api/posts', this.newPost);\n        this.posts.push(response.data);\n        this.newPost.title = '';\n        this.newPost.content = '';\n      } catch (error) {\n        console.error('Failed to create post:', error);\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* Stil kodlarÄ± buraya */\n</style>\n"],"mappings":";AAyBA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,GAAG,EAAE,4BAA4B;MACjCC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;QACPC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAM,IAAI,CAACC,MAAM,CAACC,GAAG,CAAC,iCAAiC,CAAC;QACzE,IAAI,CAACR,KAAI,GAAIM,QAAQ,CAACR,IAAI;MAC5B,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD;IACF,CAAC;IACD,MAAME,UAAUA,CAAA,EAAG;MACjB,IAAI;QACF,MAAML,QAAO,GAAI,MAAM,IAAI,CAACC,MAAM,CAACK,IAAI,CAAC,iCAAiC,EAAE,IAAI,CAACX,OAAO,CAAC;QACxF,IAAI,CAACD,KAAK,CAACa,IAAI,CAACP,QAAQ,CAACR,IAAI,CAAC;QAC9B,IAAI,CAACG,OAAO,CAACC,KAAI,GAAI,EAAE;QACvB,IAAI,CAACD,OAAO,CAACE,OAAM,GAAI,EAAE;MAC3B,EAAE,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,wBAAwB,EAAEA,KAAK,CAAC;MAChD;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}